{
    "version": 2,
    "builds": [
      {
        "src": "dynamic.html",
        "use": "@vercel/static"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/dynamic.html"
      },
      {
        "src": "/css/(.*)",
        "dest": "/css/$1"
      },
      {
        "src": "/dist/(.*)",
        "dest": "/dist/$1"
      }
    ]
  }
  